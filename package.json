{
  "name": "expo-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Expo development, project initialization, EAS services, and development assistance",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx --conditions=development src/index.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "expo",
    "react-native",
    "mobile",
    "development",
    "eas",
    "cursor"
  ],
  "author": "Mobile DevRel Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "fast-glob": "^3.3.2",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2",
    "chokidar": "^3.5.3",
    "gray-matter": "^4.0.3",
    "marked": "^9.1.6",
    "turndown": "^7.1.2",
    "semver": "^7.5.4",
    "yaml": "^2.3.4",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "bin": {
    "expo-mcp": "./dist/index.js"
  }
} 