{
  "dashboard": {
    "id": null,
    "title": "Expo MCP Server Overview",
    "tags": ["expo", "mcp", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"expo-mcp-server\"}",
            "legendFormat": "MCP Server"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis"
          },
          {
            "expr": "up{job=\"typesense\"}",
            "legendFormat": "Typesense"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Time P95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(expo_mcp_stdio_latency_ms_bucket[5m]))",
            "legendFormat": "Stdio Latency"
          },
          {
            "expr": "histogram_quantile(0.95, rate(expo_mcp_search_latency_ms_bucket[5m]))",
            "legendFormat": "Search Latency"
          },
          {
            "expr": "histogram_quantile(0.95, rate(expo_mcp_sdk_lookup_latency_ms_bucket[5m]))",
            "legendFormat": "SDK Lookup"
          },
          {
            "expr": "histogram_quantile(0.95, rate(expo_mcp_tool_execution_latency_ms_bucket[5m]))",
            "legendFormat": "Tool Execution"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "expo_mcp_memory_usage_mb{type=\"heap_used\"}",
            "legendFormat": "Memory Usage (MB)"
          },
          {
            "expr": "expo_mcp_cpu_usage_percent",
            "legendFormat": "CPU Usage (%)"
          }
        ],
        "yAxes": [
          {
            "label": "Usage",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "expo_mcp_cache_hit_rate{cache_type=\"memory\"}",
            "legendFormat": "Hit Rate (%)"
          },
          {
            "expr": "rate(expo_mcp_cache_hits_total[5m])",
            "legendFormat": "Hits/sec"
          },
          {
            "expr": "rate(expo_mcp_cache_misses_total[5m])",
            "legendFormat": "Misses/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Request Volume",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(expo_mcp_requests_total[5m])",
            "legendFormat": "Total Requests/sec"
          },
          {
            "expr": "rate(expo_mcp_successful_requests_total[5m])",
            "legendFormat": "Successful/sec"
          },
          {
            "expr": "rate(expo_mcp_failed_requests_total[5m])",
            "legendFormat": "Failed/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "expo_mcp_error_rate{time_window=\"5m\"}",
            "legendFormat": "Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 2
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Active Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{job=\"expo-mcp-server\"}",
            "legendFormat": "{{alertname}} - {{severity}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1
  }
} 